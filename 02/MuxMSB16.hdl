/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP MuxMSB16 {
    IN a[16], b[16], sel;
    OUT out;

    PARTS:
    // Put your code here:
    Mux(a=a[15], b=b[15], sel=sel, out=out);
}